@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Create Student</h2>
        <p>@Html.ActionLink("Create Student", "CreateStudent", "Student")</p>
    </div>
    <div class="col-md-4">
        <h2>List Students</h2>
        <p>@Html.ActionLink("Student Details", "StudentDetails", "Student")</p>
    </div>
    <div class="col-md-4">
        <h2>Edit Student</h2>
        <p>@Html.ActionLink("Edit Student", "EditStudent", "Student")</p>
    </div>
    <div class="col-md-4">
        <h2>Delete Student</h2>
        <p>@Html.ActionLink("Delete Student", "DeleteStudent", "Student")</p>
    </div>
</div>